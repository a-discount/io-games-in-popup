{
   "manifest_version" :2,
   "name": "IO Games in Popup",
   "version": "0.4.99999.999999",
   "content_scripts": [
      {
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "io.png",
        "default_popup": "popup.html",
        "default_title": " "
    }
}
